uDAUJC,MAVI;AAAA;AAAA;AAAA;AAAA;AAAA;AASLL,gBAAAA,MATK,SASLA,MATK;AAAA;AAAA,uBAWkB5C,OAAO,CAAC,CAAC,CAAC,KAAD,EAAQiD,MAAR,CAAD,CAAD,CAXzB;;AAAA;AAWCF,gBAAAA,QAXD;AAYLH,gBAAAA,MAAM,CAAC,WAAD,EAAcG,QAAd,CAAN;AAZK,kDAaEA,QAAQ,CAAC,CAAD,CAbV;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,GA3DmB;AA2E5BG,EAAAA,OAAO,EAAE;AA3EmB,CAAf,CAAf","sourcesContent":["import Vue from 'vue'\nimport Vuex from 'vuex'\nimport beerApi from '../services/punkapi.service'\n\nVue.use(Vuex)\n\nexport default new Vuex.Store({\n  state: {\n    beers: [],\n    beersparams: {\n      abv_gt: null,\n      abv_lt: null,\n      ibu_gt: null,\n      ibu_lt: null,\n      ebc_gt: null,\n      ebc_lt: null,\n      beer_name: null,\n      yeast: null,\n      brewed_before: null,\n      brewed_after: null,\n      hops: null,\n      malt: null,\n      food: null,\n      ids: null\n    },\n    pagination: {\n      page: 1,\n      per_page: 30\n    },\n    orderBy: 'name'\n  },\n  getters: {\n    // Return one beer from state by ID\n    getBeerById(state) {\n      return (id) => {\n        return state.beers.find(b => b.id === id)\n      }\n    },\n    // Return an ordered array of beers\n    getBeers(state) {\n      return state.beers.sort((a, b) => (a[state.orderBy] > b[state.orderBy]) ? 1 : -1)\n    }\n  },\n  mutations: {\n    //Save beers in state\n    setBeers (state, beers) {\n      state.beers = beers\n    },\n    //Merge one beer into the array\n    mergeBeer (state, beer) {\n      state.beers.concat(beer)\n    },\n    //Merge pagination options\n    setPagination (state, pagination) {\n      state.pagination = Object.assign({}, state.pagination, pagination)\n    },\n    //Merge beer params options\n    setBeersParams(state, beersparams) {\n      state.beersparams = Object.assign({}, state.beersparams, beersparams)\n    },\n    //Save sorting option\n    setOrderBy (state, orderBy) {\n      state.orderBy = orderBy\n    }\n  },\n  actions: {\n    // Fetch beers with store params\n    async fetchBeers ({state, commit}) {\n      const parameters = Object.entries(state.beersparams)\n      const response = await beerApi(parameters)\n      commit('setBeers', response)\n    },\n    // Fetch one beer if needed\n    async fetchBeer({\n      commit\n    }, beerId) {\n      const response = await beerApi([['ids', beerId]])\n      commit('mergeBeer', response)\n      return response[0]\n    }\n  },\n  modules: {\n  }\n})\n"]}]}